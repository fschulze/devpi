<metal:block use-macro="main_template" tal:define="main_class 'project'">
    <metal:title fill-slot="title">devpi - ${title}</metal:title>
    <metal:contentheader fill-slot="content_header">
        <metal:macros tal:repeat="name request.registry['macros'].get_group('project_header')" use-macro="macros[name]" />
    </metal:contentheader>
    <metal:content fill-slot="content">
        <metal:macros tal:repeat="name request.registry['macros'].get_group('project')" use-macro="macros[name]" />
    </metal:content>
</metal:block>
